    <com.qf.moveevent.MoveView
        android:id="@+id/moveview2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="#0051ff"
        android:layout_weight="1"/>

</com.qf.moveevent.MyLayout>
